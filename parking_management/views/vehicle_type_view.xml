<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="vehicle_type_view_tree" model="ir.ui.view">
		<field name="name">vehicel.type.view.tree</field>
		<field name="model">vehicel.type</field>
		<field name="arch" type="xml">
			<tree>
				<field  name="name"/>
			</tree>
		</field>
	</record>
	<record id="vehicle_type_view_form" model="ir.ui.view">
		<field name="name">vehicel.type.view.form</field>
		<field name="model">vehicel.type</field>
		<field name="arch" type="xml">
			<form string="User Data">
				<sheet>
					<group>
						<field  name="name"/>
					</group>					
				</sheet>
			</form>
		</field>
	</record>
	
	<record id="action_vehicle_type_view" model="ir.actions.act_window">
		<field name="name">Vehicle Type</field>
		<field name="res_model">vehicel.type</field>
		<field name="view_mode">tree,form</field>
	</record>

    <menuitem id="sub_menu_vehicle_type"
            name="Vehicle Type"
            parent="sub_menu_parking_data"
            action="action_vehicle_type_view"
            sequence="2"/>
</odoo>