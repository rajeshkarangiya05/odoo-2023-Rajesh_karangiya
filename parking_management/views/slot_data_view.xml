<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="slot_data_view_tree" model="ir.ui.view">
		<field name="name">slot.data.view.tree</field>
		<field name="model">slot.data</field>
		<field name="arch" type="xml">
			<tree>
				<field  name="base_name_id"/>
				<field  name="section_name"/>
				<field  name="section_partition"/>
				<field  name="vehicel_type_id"/>
				<field  name="strength"/>
				<field  name="booked"/>
				<field  name="token_user"/>


			</tree>
		</field>
	</record>
	<record id="slot_data_view_form" model="ir.ui.view">
		<field name="name">slot.data.view.form</field>
		<field name="model">slot.data</field>
		<field name="arch" type="xml">
			<form string="User Data">
				<sheet>
					<group>
						<field  name="base_name_id"/>
						<field  name="section_name"/>
						<field  name="section_partition"/>
						<field  name="vehicel_type_id"/>
						<field  name="strength"/>						
						<field  name="booked"/>
						<field  name="token_user"/>

					</group>					
				</sheet>
			</form>
		</field>
	</record>
	
	<record id="action_slot_data_view" model="ir.actions.act_window">
		<field name="name">Slot Data</field>
		<field name="res_model">slot.data</field>
		<field name="view_mode">tree,form</field>
	</record>

    <menuitem id="sub_menu_slot_data"
            name="Slot Data"
            parent="sub_menu_parking_data"
            action="action_slot_data_view"
            sequence="4"/>
</odoo>