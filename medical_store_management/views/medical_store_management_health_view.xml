<?xml version="1.0" encoding="utf-8" ?>
<odoo>
	<record id="health_symptoms_view_tree" model="ir.ui.view">
		<field name="name">health.symptoms.tree</field>
        <field name="model">health.symptoms</field>
        <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                </tree>
        </field>
    </record>
    <record id="health_symptoms_view_form" model="ir.ui.view">
        <field name="name">health.symptoms.form</field>
        <field name="model">health.symptoms</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="name"/>
                </group>
            </form>
        </field>
    </record> 

    <record id="medical_store_management_health_view" model="ir.actions.act_window">
        <field name="name">Health Symptoms</field>
        <field name="res_model">health.symptoms</field>
        <field name="view_mode">tree,form</field>
    </record>
    <menuitem id="health_symptoms"
            name="Health Symptoms"
            action="medical_store_management_health_view"
            parent="medicine_information_medical"
            sequence="1"/>

    
</odoo>